<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceContainer name="content">
            <block class="Magento\Cms\Block\Block" name="ad_home_top">
                <arguments>
                    <argument name="block_id" xsi:type="string">ad_home_top</argument>
                </arguments>
            </block>
			<block class="Magento\Cms\Block\Block" name="add_home_top">
                <arguments>
                    <argument name="block_id" xsi:type="string">add_home_top</argument>
                </arguments>
	        </block>
	        <block class="Magento\Cms\Block\Block" name="add_home_middle">
                <arguments>
                    <argument name="block_id" xsi:type="string">add_home_middle</argument>
                </arguments>
	        </block>
	        <block class="Magento\Cms\Block\Block" name="add_home_bottom">
                <arguments>
                    <argument name="block_id" xsi:type="string">add_home_bottom</argument>
                </arguments>
	        </block>
	        <block class="Magento\Cms\Block\Block" name="customer_show" after="-">
                <arguments>
                    <argument name="block_id" xsi:type="string">customer_show</argument>
                </arguments>
	        </block>
	    </referenceContainer>
    </body>
</page>
