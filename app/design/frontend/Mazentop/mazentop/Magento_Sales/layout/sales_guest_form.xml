<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Cms\Block\Block" name="track_order_desc" before="-" >
                <arguments>
                    <argument name="block_id" xsi:type="string">track_order_desc</argument>
                </arguments>
            </block>
            <block class="Magento\Sales\Block\Widget\Guest\Form" name="guest.form" template="Magento_Sales::guest/form.phtml"/>
        </referenceContainer>
    </body>
</page>
